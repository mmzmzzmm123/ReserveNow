round-color: $primary-color;\r\n          border-radius: 50%;\r\n          transition: all 0.3s;\r\n        }\r\n        \r\n        &:hover {\r\n          padding-left: 22px;\r\n          \r\n          &::before {\r\n            width: 10px;\r\n            height: 10px;\r\n            background: $gradient-primary;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.footer-newsletter {\r\n  p {\r\n    color: rgba(255, 255, 255, 0.7);\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .newsletter-form {\r\n    display: flex;\r\n    margin-bottom: 10px;\r\n    \r\n    .el-input {\r\n      :deep(.el-input__wrapper) {\r\n        border-radius: 4px 0 0 4px;\r\n        background-color: rgba(255, 255, 255, 0.1);\r\n        box-shadow: none;\r\n        border: none;\r\n        \r\n        .el-input__inner {\r\n          color: white;\r\n          \r\n          &::placeholder {\r\n            color: rgba(255, 255, 255, 0.5);\r\n          }\r\n        }\r\n      }\r\n    }\r\n    \r\n    .el-button {\r\n      border-radius: 0 4px 4px 0;\r\n      background: $gradient-primary;\r\n      border: none;\r\n      box-shadow: none;\r\n      \r\n      &:hover {\r\n        background: linear-gradient(135deg, darken($primary-color, 5%), #ff5252);\r\n      }\r\n    }\r\n  }\r\n  \r\n  .newsletter-info {\r\n    font-size: 12px;\r\n    color: rgba(255, 255, 255, 0.5);\r\n    margin-top: 10px;\r\n  }\r\n}\r\n\r\n.footer-bottom {\r\n  background-color: $footer-bg-secondary;\r\n  padding: 25px 0;\r\n  \r\n  .footer-bottom-content {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    \r\n    .copyright {\r\n      color: rgba(255, 255, 255, 0.7);\r\n    }\r\n    \r\n    .footer-cards {\r\n      display: flex;\r\n      gap: 10px;\r\n      \r\n      .payment-card {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        width: 40px;\r\n        height: 25px;\r\n        background: rgba(255, 255, 255, 0.1);\r\n        border-radius: 4px;\r\n        color: rgba(255, 255, 255, 0.7);\r\n        \r\n        &.visa {\r\n          color: #1434CB;\r\n          background: white;\r\n        }\r\n        \r\n        &.mastercard {\r\n          color: #FF5F00;\r\n          background: white;\r\n        }\r\n        \r\n        &.amex {\r\n          color: #2E77BB;\r\n          background: white;\r\n        }\r\n        \r\n        &.paypal {\r\n          color: #253B80;\r\n          background: white;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.footer-app-badge {\r\n  position: fixed;\r\n  bottom: 20px;\r\n  right: 20px;\r\n  z-index: 99;\r\n  \r\n  .badge-content {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    background: $gradient-primary;\r\n    color: white;\r\n    padding: 12px 20px;\r\n    border-radius: 50px;\r\n    box-shadow: 0 5px 15px rgba($primary-color, 0.3);\r\n    cursor: pointer;\r\n    transition: all 0.3s;\r\n    \r\n    &:hover {\r\n      transform: translateY(-3px);\r\n      box-shadow: 0 8px 20px rgba($primary-color, 0.4);\r\n    }\r\n    \r\n    .el-icon {\r\n      font-size: 22px;\r\n    }\r\n    \r\n    .badge-text {\r\n      display: flex;\r\n      flex-direction: column;\r\n      line-height: 1.2;\r\n      \r\n      span {\r\n        font-size: 10px;\r\n        opacity: 0.9;\r\n      }\r\n      \r\n      strong {\r\n        font-size: 14px;\r\n        font-weight: 600;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .footer-top {\r\n    padding: 50px 0 30px;\r\n    \r\n    .footer-row {\r\n      flex-direction: column;\r\n      gap: 40px;\r\n    }\r\n  }\r\n  \r\n  .footer-bottom {\r\n    .footer-bottom-content {\r\n      flex-direction: column;\r\n      gap: 15px;\r\n      text-align: center;\r\n    }\r\n  }\r\n  \r\n  .footer-brand {\r\n    .logo {\r\n      justify-content: center;\r\n    }\r\n    \r\n    .brand-description {\r\n      max-width: none;\r\n      text-align: cen