ials Section -->\r\n    <section class=\"testimonials-section\">\r\n      <h2 class=\"section-title\">User Reviews</h2>\r\n      <div class=\"testimonials-carousel\">\r\n        <el-carousel :interval=\"4000\" type=\"card\">\r\n          <el-carousel-item v-for=\"testimonial in testimonials\" :key=\"testimonial.id\">\r\n            <div class=\"testimonial-card\">\r\n              <div class=\"testimonial-content\">\r\n                <div class=\"quote-icon\">\r\n                  <el-icon><ChatRound /></el-icon>\r\n                </div>\r\n                <p class=\"testimonial-text\">{{ testimonial.content }}</p>\r\n                <div class=\"testimonial-author\">\r\n                  <img :src=\"testimonial.avatar\" :alt=\"testimonial.name\" class=\"author-avatar\">\r\n                  <div class=\"author-info\">\r\n                    <h4>{{ testimonial.name }}</h4>\r\n                    <p>{{ testimonial.title }}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- CTA Section -->\r\n    <section class=\"cta-section\">\r\n      <div class=\"cta-content\">\r\n        <h2>Start Your Culinary Journey</h2>\r\n        <p>Register now and enjoy convenient dining experiences</p>\r\n        <el-button type=\"primary\" size=\"large\" @click=\"goToRegister\">\r\n          Free Registration\r\n          <el-icon class=\"el-icon--right\"><ArrowRight /></el-icon>\r\n        </el-button>\r\n      </div>\r\n    </section>\r\n  </div>\r\n  <AppFooter />\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport AppNavbar from '@/components/AppNavbar.vue';\r\nimport AppFooter from '@/components/AppFooter.vue';\r\nimport { \r\n  Calendar, \r\n  Star, \r\n  Discount, \r\n  Service, \r\n  ChatRound,\r\n  ArrowRight\r\n} from '@element-plus/icons-vue';\r\n\r\