vice;\n      },\n      get ChatRound() {\n        return ChatRound;\n      },\n      get ArrowRight() {\n        return ArrowRight;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","useRouter","AppNavbar","AppFooter","Calendar","Star","Discount","Service","ChatRound","ArrowRight","router","testimonials","id","content","name","title","avatar","goToRegister","push"],"sources":["/Users/taoyuxuan/Documents/RestaurantReservation/qd/src/views/Explore.vue"],"sourcesContent":["<template>\r\n  <AppNavbar />\r\n  <div class=\"explore-page\">\r\n    <!-- Hero Section -->\r\n    <section class=\"hero-section\">\r\n      <div class=\"hero-content\">\r\n        <h1 class=\"hero-title\">Explore the Infinite Possibilities of Food</h1>\r\n        <p class=\"hero-subtitle\">Make every meal a memorable experience</p>\r\n        <div class=\"hero-stats\">\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-number\">1000+</span>\r\n            <span class=\"stat-label\">Curated Restaurants</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-number\">500K+</span>\r\n            <span class=\"stat-label\">Trusted Users</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-number\">98%</span>\r\n            <span class=\"stat-label\">Satisfaction Rate</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"hero-decoration\"></div>\r\n    </section>\r\n\r\n    <!-- Features Section -->\r\n    <section class=\"features-section\">\r\n      <h2 class=\"section-title\">Why Choose Us</h2>\r\n      <div class=\"features-grid\">\r\n        <div class=\"feature-card\">\r\n          <div class=\"feature-icon\">\r\n            <el-icon><Calendar /></el-icon>\r\n          </div>\r\n          <h3>Easy Booking</h